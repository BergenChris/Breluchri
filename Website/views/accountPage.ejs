<%- include("partials/header") %>

<main class="col-12 col-md-9 mx-auto">
    <h1 class="text-center resetfont mx-auto bg-opacity-75">Top Scores</h1>
    <div class="row">
        <div class="col-12 col-md-6 mb-3">
            <h2 class="text-center">Sudden Death (SD)</h2>
            <ol class="list-group list-group-flush">
                <% for (let i = 0; i < sdScores.length; i++) { %>
                <li class="list-group-item bg-transparent">
                    <%= sdScores[i] %>
                </li>
                <% } %>
            </ol>
        </div>
        <div class="col-12 col-md-6 mb-3">
            <h2 class="text-center">10 Rounds (10R)</h2>
            <ol class="list-group list-group-flush">
                <% for (let i = 0; i < r10Scores.length; i++) { %>
                <li class="list-group-item bg-transparent">
                    <%= r10Scores[i] %>
                </li>
                <% } %>
            </ol>
        </div>
    </div>
</main>